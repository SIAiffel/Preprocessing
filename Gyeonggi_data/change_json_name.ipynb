    "import os\n",
    "  \n",
    "files = os.listdir('./')\n",
    "  \n",
    "for file in files:\n",
    "    if file[-2:] != 'py':\n",
    "        name = file\n",
    "        new_name = file.replace('원래 글자', '바꿀 글자')\n",
    "        name = os.path.join('./', name)\n",
    "        new_name = os.path.join('./', new_name)\n",
    "        os.rename(name, new_name)"
